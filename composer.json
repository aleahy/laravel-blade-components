{
    "name": "aleahy/laravel-blade-components",
    "description": "Blade components for use in Livewire Laravel projects",
    "keywords": [
        "aleahy",
        "laravel",
        "laravel-blade-components"
    ],
    "homepage": "https://github.com/aleahy/laravel-blade-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Leahy",
            "email": "alex@languagetub.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Aleahy\\LaravelBladeComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aleahy\\LaravelBladeComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aleahy\\LaravelBladeComponents\\LaravelBladeComponentsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
